import (
  Data.Numeric.Nat
) -> (
  while: 
    max_loops@Nat
      -> init_state@state_type
      -> cond@(state_type->Bool) 
      -> (state_type -> state_type) 
      -> Maybe state_type,
  unsafe_while: 
    init_state@state_type 
      -> (state_type -> Bool) 
      -> (state_type -> state_type) 
      -> state_type
)