Imperative:
  ->  import list_module:$base.linkedlist 
  ->  define List:$list_module.list   
  ->  define Program_state:Any
  ->  define Instruction:(
        given input@$Program_state
        -> return output@$Program_state
      )
  ->  define Program:$List[$Instruction]
  ->  define Execute:
        given program@$Program
        -> given initial_state@$Program_state
        -> 
  ->  return ()