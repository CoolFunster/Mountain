//https://www.investopedia.com/terms/b/balancesheet.asp
BalanceSheet: 
    import DollarAmount
    -> return {
        balancesheet:(
            asset:DollarAmount,
            liabilities:DollarAmount,
            equity:DollarAmount
        ) 
         | $balancesheet.assets == $balancesheet.liabilities + $balancesheet.equity
    }